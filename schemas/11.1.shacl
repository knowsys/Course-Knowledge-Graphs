eg:CoordinateShape
  a sh:NodeShape ;
  sh:targetClass wikibase:GlobecoordinateValue ;
  sh:property [
    sh:path wikibase:geoLatitude ;
    sh:datatype xsd:double
  ] ;
  sh:property [
    sh:path wikibase:geoLongitude ;
    sh:datatype xsd:double
  ] ;
  sh:property [
    sh:path wikibase:geoPrecision ;
    sh:datatype xsd:double
  ] ;
  sh:property [
    sh:path wikibase:geoGlobe ;
    sh:nodeKind sh:IRI
  ] ;
  sh:closed true .